{
  "local": {
    "label": "local-container",
    "image": "r3r57/montreal:latest-arm",
    "device": "/dev/ttyUSB0",
    "network_name": "nsq-net",
    "environment": {
      "SERVICE": "sensor"
    }
  },
  "metadata": {
    "hostname_path": "/etc/pi-hostname",
    "machine_id_path": "/etc/machine-id",
    "location_info_path": "/etc/location.info"
  },
  "sensors": {
    "ash2200": {
      "ids": "",
      "device": "/dev/ttyUSB0",
      "baudrate": "9600",
      "timeout": "200"
    },
    "interval": 30
  },
  "nsq": {
    "nsqd": {
      "ip": "http://nsqd",
      "name": "nsqd",
      "port": "4151",
      "timeout": 60
    },
    "nsqlookupd": {
      "ip": "http://nsqlookupd",
      "name": "nsqlookupd",
      "port": "4161",
      "interval": 300,
      "timeout": 60
    },
    "topics": {
      "data_topic": "sensors"
    },
    "writer":{
      "timeout": 10,
      "max_tries": 10
    }
  },
  "memcached": {
    "ip": "memcached",
    "port": 11211,
    "connect_timeout": 180,
    "timeout": 180,
    "key_expiration": 180,
    "noreply_flag": true
  },
  "logging": {
    "level": "debug",
    "file": "",
    "format": "[%(asctime)s] %(levelname)-8s %(message)s"
  }
}
